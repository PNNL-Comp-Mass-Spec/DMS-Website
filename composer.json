{
    "name": "pnnl-comp-mass-spec/dms_website",
    "type": "project",
    "description": "PNNL Omics Data Management System",
    "homepage": "https://dms2.pnl.gov",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "codeigniter4/framework": "^4.4",
        "phpoffice/phpspreadsheet": "^1.29.2"
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "@composer dump-autoload"
        ],
        "post-update-cmd": [
            "@composer dump-autoload"
        ]
    }
}

{
    "name": "pnnl-comp-mass-spec/dms_website",
    "description": "PNNL Omics Data Management System",
    "license": "MIT",
    "type": "project",
    "homepage": "https://dms2.pnl.gov",
    "require": {
        "php": "^8.2",
        "codeigniter4/framework": "^4.4",
        "phpoffice/phpspreadsheet": "^1.29.10"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "codeigniter/phpstan-codeigniter": "^1.5",
        "phpstan/extension-installer": "^1.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer dump-autoload"
        ],
        "post-update-cmd": [
            "@composer dump-autoload"
        ]
    }
}
